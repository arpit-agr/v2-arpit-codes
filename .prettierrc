{
	"printWidth": 90,
	"tabWidth": 2,
	"singleQuote": true,
	"bracketSpacing": false,
	"quoteProps": "consistent",
	"trailingComma": "none",
	"arrowParens": "avoid",
	"useTabs": true,
	"plugins": ["prettier-plugin-jinja-template"],
	"overrides": [
		{
			"files": ["*.html"],
			"options": {
				"parser": "jinja-template"
			}
		}
	]
}
